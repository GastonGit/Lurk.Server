{
  "name": "server",
  "version": "3.0.10",
  "private": true,
  "scripts": {
    "start": "set NODE_ENV=production&&ts-node -r dotenv/config src/app.ts",
    "start:watch": "nodemon",
    "lint": "eslint --ext .ts,.tsx",
    "lint:fix": "eslint --ext .ts,.tsx --fix",
    "dev": "set NODE_ENV=development&&ts-node -r dotenv/config src/app.ts",
    "test": "set NODE_ENV=test&&mocha",
    "test-with-coverage": "set NODE_ENV=test&&nyc mocha --slow 0",
    "prepare": "husky install"
  },
  "dependencies": {
    "config": "^3.3.6",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.6",
    "proxyquire": "^2.1.3",
    "tmi.js": "^1.7.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/config": "^0.0.40",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.6.2",
    "@types/node-fetch": "^2.5.12",
    "@types/sinon": "^10.0.6",
    "@types/tmi.js": "^1.8.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "chai": "^4.3.3",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.4",
    "mocha": "^8.3.1",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "sinon": "^12.0.1",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
